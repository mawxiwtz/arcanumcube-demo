{
  "name": "arcanumcube-demo",
  "version": "0.2.0",
  "description": "Arcanum Cube Demo",
  "license": "MIT",
  "author": "mawxiwtz",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mawxiwtz/arcanumcube-demo.git"
  },
  "keywords": [
    "rubiks-cube",
    "puzzle",
    "three",
    "webgl",
    "ai",
    "machine-learning"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/demo.js",
  "scripts": {
    "lint": "eslint .",
    "dev": "vite",
    "build": "npm run build:dev && npm run build:minify && npm run build:demo",
    "build:dev": "vite build && rollup -c rollup.config.dts.js",
    "build:minify": "node scripts/minify.mjs",
    "build:demo": "vite build --config vite.config.demo.ts",
    "clean": "rimraf dist",
    "preview": "vite preview",
    "doc": "typedoc --entryPointStrategy expand ./src --out ./docs --disableSources",
    "test": "jest"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tweenjs/tween.js": "^25.0.0",
    "arcanumcube": "^0.2.2",
    "three": "^0.181.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/three": "^0.181.0",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "rimraf": "^6.1.0",
    "rollup": "^4.53.2",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.4.5",
    "typedoc": "^0.28.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2"
  }
}
