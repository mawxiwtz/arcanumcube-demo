{
  "name": "arcanumcube-demo",
  "version": "0.2.2",
  "description": "Arcanum Cube Demo",
  "license": "MIT",
  "author": "mawxiwtz",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mawxiwtz/arcanumcube-demo.git"
  },
  "keywords": [
    "rubiks-cube",
    "puzzle",
    "three",
    "webgl",
    "ai",
    "machine-learning"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/demo.js",
  "scripts": {
    "lint": "eslint .",
    "dev": "vite",
    "build": "npm run build:dev && npm run build:minify && npm run build:demo",
    "build:dev": "vite build && rollup -c rollup.config.dts.js",
    "build:minify": "node scripts/minify.mjs",
    "build:demo": "vite build --config vite.config.demo.ts",
    "clean": "rimraf dist",
    "preview": "vite preview",
    "doc": "typedoc --entryPointStrategy expand ./src --out ./docs --disableSources",
    "test": "jest"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tweenjs/tween.js": "^25.0.0",
    "arcanumcube": "^0.2.3",
    "glob": "^13.0.6",
    "three": "^0.183.1"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.1",
    "@types/three": "^0.183.1",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "rimraf": "^6.1.3",
    "rollup": "^4.59.0",
    "rollup-plugin-dts": "^6.3.0",
    "ts-jest": "^29.4.6",
    "typedoc": "^0.28.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^7.3.1"
  }
}
